sudo qemu-system-x86_64 -daemonize -display none -enable-kvm -machine accel=kvm -smp cores=2 -m 4096 \
  -hda /home/xrvr/xrvr-images/XRa.vmdk \
  -serial telnet::9101,server,nowait -serial telnet::9102,server,nowait \
  -net nic,model=virtio,vlan=0,macaddr=00:22:00:ff:0A:00 -net tap,vlan=0,script=/etc/qemu-ifup \
  -net nic,model=virtio,vlan=1,macaddr=00:22:00:ff:0A:01 -net socket,vlan=1,connect=127.0.0.1:14000 \
  -net nic,model=virtio,vlan=2,macaddr=00:22:00:ff:0A:02 -net socket,vlan=2,connect=127.0.0.1:15000 \
  -net nic,model=virtio,vlan=3,macaddr=00:22:00:ff:0A:03 -net socket,vlan=3,connect=127.0.0.1:16000 \
  -net nic,model=virtio,vlan=4,macaddr=00:22:00:ff:0A:04 -net socket,vlan=4,connect=127.0.0.1:19000
